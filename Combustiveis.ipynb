   "source": [
    "#Substitui siglas pelo nome da região\n",
    "regioes = {'N':'Norte', 'NE':'Nordeste', 'CO':'Centro-Oeste', 'S':'Sudeste', 'SE':'Suldeste'}\n",
    "df['Regiao'] = df['Regiao'].replace(regioes)"
   ]
