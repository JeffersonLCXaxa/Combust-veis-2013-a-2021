    "df['Valor de Compra'] = df['Valor de Compra'].str.replace(',', '.').astype(float)\n",
    "df['Valor de Venda'] = df['Valor de Venda'].str.replace(',', '.').astype(float)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "fbca4151",
   "metadata": {},
   "outputs": [],
   "source": [
